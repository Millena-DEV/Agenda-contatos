<?php

class ClienteModel
{
    public $idCliente,$nome,$idPessoa_juridica,$idPessoa_Fisica,$idEndereco;
public $rows;
    public function save(){
        include 'DAO/ClienteDAO.php';

        $dao = new ClienteDAO();

        $dao -> insert($this);
    }
public function getAllRows(){
    include 'DAO/ClienteDAO.php';
    $dao= new ClienteDAO;
   $this->rows-> $dao->select();
}




}
